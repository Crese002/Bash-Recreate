echo A && echo B
echo A || echo B		# only print A
echo A; echo B
ls -j && echo A			# no output
ls -j || echo A			# only print A
ls -j; echo A			# only print A
ls -j && echo A || echo B	# only print B
echo A || echo B || echo C	# only print A
echo A || echo B && echo C	# only print A and C
echo A && echo B && echo C  # print A, B, C
